    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.30",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/config": "0.0.36",
    "@types/cors": "^2.8.6",
    "@types/dotenv-safe": "^8.1.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
