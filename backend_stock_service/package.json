    "@types/workerpool": "^5.0.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "csv": "^5.3.2",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "node-cache": "^5.1.0",
    "prettyjson": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "sequelize": "^5.21.7",
    "sharp": "^0.25.2",
    "winston": "^3.2.1",
    "workerpool": "^6.0.0",
    "ws": "^7.3.0"
  },
