{
  "name": "backend_stock_service",
  "version": "1.0.0",
  "description": "stock photo service backend",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf build && tsc --build tsconfig.prod.json",
    "start:api": "env NODE_ENV=production UV_THREADPOOL_SIZE=128 node build/api.js",
    "start:imageproc": "env NODE_ENV=production UV_THREADPOOL_SIZE=128 node build/imageproc.js",
    "start:fileserver": "env NODE_ENV=production UV_THREADPOOL_SIZE=128 node build/fileserver.js",
    "deploy:api:dev": "env NODE_ENV=dev ts-node-dev --respawn src/api.ts",
    "deploy:imageproc:dev": "env NODE_ENV=dev ts-node-dev --respawn src/imageproc.ts",
    "deploy:fileserver:dev": "env NODE_ENV=dev ts-node-dev --respawn src/fileserver.ts"
  },
  "author": "rajatkb",
  "license": "ISC",
